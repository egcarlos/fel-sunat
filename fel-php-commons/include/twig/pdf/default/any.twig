<page backtop="55mm" backbottom="25mm" style="font-family:Arial">
    <page_header>
        {% include [ spec ~ '/' ~ type ~ '.header.twig', spec ~ '/header.twig', 'default/' ~ type ~ '.header.twig', 'default/header.twig'] %}
        <br />
        {% include [ spec ~ '/' ~ type ~ '.subheader.twig', spec ~ '/subheader.twig', 'default/' ~ type ~ '.subheader.twig', 'default/subheader.twig'] %}
    </page_header>
    <page_footer>
        {% include [ spec ~ '/' ~ type ~ '.footer.twig', spec ~ '/footer.twig', 'default/' ~ type ~ '.footer.twig', 'default/footer.twig'] %}
    </page_footer>
    {% include [ spec ~ '/' ~ type ~ '.body.twig', spec ~ '/body.twig', 'default/' ~ type ~ '.body.twig', 'default/body.twig'] %}
</page>